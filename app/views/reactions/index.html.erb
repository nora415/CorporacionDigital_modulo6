<p style="color: green"><%= notice %></p>

<% content_for :title, "Reactions" %>

<h1>Reactions</h1>

<div id="reactions">
  <% @reactions.each do |reaction| %>
    <%= render reaction %>
    <p>
      <%= link_to "Show this reaction", reaction %>
    </p>
  <% end %>
</div>

<%= link_to "New reaction", new_reaction_path %>
